<%- include('inc/head', data) %>
<%- include('inc/pageTop', data) %>
<table>
	<thead>
		<tr>
			<th>Name</th>
		</tr>
	</thead>
	<tbody>
	<% _.each(data.cmsSnippets, function (snippet) { %>
		<tr>
			<td><a href="adminCmsSnippetEdit?langs=<% if(data.global.langs){print(data.global.langs.join(','));}else{print('en');} %>&name=<%= snippet.name %>"><%= snippet.name %></a></td>
		</tr>
	<% }) %>
	</tbody>
</table>
<%- include('inc/pageBottom', data) %>
