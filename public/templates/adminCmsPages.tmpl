<%- include('inc/head', data) %>
<%- include('inc/pageTop', data) %>
<table>
	<thead>
		<tr>
			<th>Uuid</th>
			<th>Name</th>
			<th>Published</th>
		</tr>
	</thead>
	<tbody>
	<% _.each(data.cmsPages, function (page) { %>
		<tr>
			<td><a href="adminCmsPageEdit?langs=<% if(data.global.langs){print(data.global.langs.join(','));}else{print('en');} %>&uuid=<%= page.uuid %>"><%= page.uuid %></a></td>
			<td><%= page.name %></td>
			<td><% if (page.published) print('Yes'); else print('No'); %></td>
		</tr>
	<% }) %>
	</tbody>
</table>
<%- include('inc/pageBottom', data) %>
